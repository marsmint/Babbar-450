void sort012(int a[], int n)
{
    int l=0;
    
    int m=0;
    int r=n-1;
    while(m<=r)
    {  if(a[m]==0)
       {    swap(a[l], a[m]);
             l++;
             m++;
       }
       else if(a[m]==1)
       {   m++;
           
       }
       else if( a[m]==2)
       { swap(a[r],a[m]);
         r--;
         
       }
    }
}
